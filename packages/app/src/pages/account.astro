---
import CenterContentLayout from "~/layouts/CenterContentLayout.astro";
import PageLayout from "~/layouts/PageLayout.astro";

const session = await Astro.locals.auth.validate();
if (!session) {
  return Astro.redirect("/");
}
---

<PageLayout seo={{ title: "Account" }}>
  <CenterContentLayout size="sm">
    <h1 class="mb-4 text-center text-3xl font-bold">Update Profile</h1>
  </CenterContentLayout>
</PageLayout>
